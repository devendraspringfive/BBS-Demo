<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <validationRules>
        <fullName>DEF_WW_Prevent_Sync_of_a_WO_Without_IP</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISBLANK(SVMXC__Component__c),
				ISPICKVAL(SVMXC__Order_Status__c,&apos;Job Validated&apos;)
			)</errorConditionFormula>
        <errorMessage>The Work Order can&apos;t be synchronized without an Installed Product</errorMessage>
    </validationRules>
</CustomObject>
